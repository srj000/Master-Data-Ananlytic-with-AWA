create database demodb;
drop database demodb;
CREATE DATABASE DEMO_DB;

CREATE SCHEMA STUDENTDATA;
USE DEMO_DB.STUDENTDATA;
CREATE TABLE STUDENTS_RECORDS(
    RECORDID INT primary KEY,
    STUDENTNAME VARCHAR(30),
    SUBJECT VARCHAR(30),
    MARKS INT,
    YEAR INT
);

SELECT * FROM STUDENTS_RECORDS;

CREATE or replace TABLE STUDENTS_RECORDS_NEW(
    RECORDID INT primary KEY,
    STUDENTNAME VARCHAR(30),
    SUBJECT VARCHAR(30),
    MARKS INT,
    YEAR INT
);
SELECT * FROM DEMO_DB.STUDENTDATA.STUDENTS_RECORDS_NEW;

select * from DEMO_DB.STUDENTDATA."student_data_new"
